<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView app:id="@r/listing_matte" app:layout_width="-1" app:layout_height="-2" style="@r/CardViewElementStyle">
    <LinearLayout app:orientation="0" app:layout_width="-1" app:layout_height="-2">
        <FrameLayout app:id="@r/listing_image_region" app:layout_width="0dp" app:layout_height="-2" app:layout_weight="1.0">
            <com.etsy.android.uikit.view.ListingFullImageView app:id="@r/listing_image" app:layout_width="-1" app:layout_height="-2" app:scaleType="6" app:adjustViewBounds="false" app:contentDescription="@r/listing_image_content_description" />
            <LinearLayout app:layout_gravity="35" app:layout_width="-2" app:layout_height="-2">
                <ImageView app:id="@r/btn_fav" app:layout_width="-2" app:layout_height="-2" style="@r/IconFavoriteListing" />
                <ImageView app:id="@r/btn_lists" app:layout_width="-2" app:layout_height="-2" style="@r/IconListsListing" />
